  , "debugConsole.warningForeground"                      : "#ffd787"

  , "disabledForeground"                                  : "#5f5f5f7f"
  , "list.deemphasizedForeground"                         : "#5f5f5f7f"

  //____________________________________________________________________
  // ICONS
  //____________________________________________________________________
  , "editorLightBulb.foreground"                          : "#ffd787"

  , "symbolIcon.arrayForeground"                          : "#5faf87"
  , "symbolIcon.booleanForeground"                        : "#c6c6c6"
  , "symbolIcon.classForeground"                          : "#87d7af"
  //, "symbolIcon.colorForeground"                          : "#"
  , "symbolIcon.constantForeground"                       : "#ffd787"
  , "symbolIcon.constructorForeground"                    : "#af87d7"
  , "symbolIcon.enumeratorForeground"                     : "#af87d7"
  , "symbolIcon.enumeratorMemberForeground"               : "#ffd787"
  //, "symbolIcon.eventForeground"                          : "#"
  , "symbolIcon.fieldForeground"                          : "#87afd7"
  //, "symbolIcon.fileForeground"                           : "#"
  //, "symbolIcon.folderForeground"                         : "#"
  , "symbolIcon.functionForeground"                       : "#5fafaf"
  //, "symbolIcon.interfaceForeground"                      : "#"
  //, "symbolIcon.keyForeground"                            : "#"
  //, "symbolIcon.keywordForeground"                        : "#"
  , "symbolIcon.methodForeground"                         : "#5fafaf"
  , "symbolIcon.moduleForeground"                         : "#ffd787"
  , "symbolIcon.namespaceForeground"                      : "#af87d7"
  //, "symbolIcon.nullForeground"                           : "#"
  //, "symbolIcon.numberForeground"                         : "#"
  //, "symbolIcon.objectForeground"                         : "#"
  //, "symbolIcon.operatorForeground"                       : "#"
  //, "symbolIcon.packageForeground"                        : "#"
  , "symbolIcon.propertyForeground"                       : "#87afd7"
  //, "symbolIcon.referenceForeground"                      : "#"
  //, "symbolIcon.snippetForeground"                        : "#"
  , "symbolIcon.stringForeground"                         : "#af5faf"
  , "symbolIcon.structForeground"                         : "#ffd787"
  , "symbolIcon.textForeground"                           : "#af5faf"
