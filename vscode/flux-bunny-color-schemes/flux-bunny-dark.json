{
  "$schema"     : "vscode://schemas/color-theme"
, "name"        : "Flux Bunny Dark"
, "type"        : "dark"

, "colors":
  { "foreground"                                          : "#c6c6c6"
  , "selection.background"                                : "#af87d7"
  , "errorForeground"                                     : "#ff5f87"
  , "focusBorder"                                         : "#005f5f"
  //, "window.activeBorder"                                 : "#005f5f"
  , "widget.border"                                       : "#00875f"

  , "editor.background"                                   : "#262626"
  , "editor.foreground"                                   : "#c6c6c6"

  , "terminal.background"                                 : "#303030"
  , "terminal.foreground"                                 : "#d7d7d7"

  , "terminal.ansiBrightBlack"                            : "#121212"
  , "terminal.ansiBrightRed"                              : "#d75f87"
  , "terminal.ansiBrightGreen"                            : "#87af5f"
  , "terminal.ansiBrightYellow"                           : "#ffd787"
  , "terminal.ansiBrightBlue"                             : "#00afd7"
  , "terminal.ansiBrightMagenta"                          : "#af87af"
  , "terminal.ansiBrightCyan"                             : "#5fafaf"
  , "terminal.ansiBrightWhite"                            : "#d0d0d0"

  , "terminal.ansiBlack"                                  : "#1c1c1c"
  , "terminal.ansiRed"                                    : "#d787af"
  , "terminal.ansiGreen"                                  : "#afd787"
  , "terminal.ansiYellow"                                 : "#ffaf87"
  , "terminal.ansiBlue"                                   : "#87afd7"
  , "terminal.ansiMagenta"                                : "#af87d7"
  , "terminal.ansiCyan"                                   : "#87d7af"
  , "terminal.ansiWhite"                                  : "#d7d7d7"

  , "editorBracketHighlight.foreground1"                  : "#87af5f"
  , "editorBracketHighlight.foreground3"                  : "#5fafaf"
  , "editorBracketHighlight.foreground2"                  : "#d75f87"
  , "editorBracketHighlight.unexpectedBracket.foreground" : "#ff5f87"

  , "editor.wordHighlightBackground"                      : "#303030af"
  , "editorWarning.foreground"                            : "#ffd787"
  , "editorError.foreground"                              : "#ff5f87"
  , "editorLineNumber.activeForeground"                   : "#afd787"
  , "editorLineNumber.foreground"                         : "#878787"

  , "tab.activeBackground"                                : "#262626"
  , "tab.activeForeground"                                : "#d7d7d7"
  , "tab.inactiveBackground"                              : "#303030"
  , "tab.inactiveForeground"                              : "#c6c6c6"
  , "tab.unfocusedActiveForeground"                       : "#5fafaf"
  , "tab.unfocusedInactiveForeground"                     : "#c6c6c6"

  , "sideBar.background"                                  : "#303030"
  , "sideBar.foreground"                                  : "#c6c6c6"
  , "sideBar.border"                                      : "#afd787"
  , "sideBarTitle.background"                             : "#005f87"
  , "sideBarTitle.foreground"                             : "#c6c6c6"
  , "sideBarSectionHeader.background"                     : "#875faf"
  , "sideBarSectionHeader.foreground"                     : "#c6c6c6"

  , "panel.background"                                    : "#303030"
  , "panel.border"                                        : "#afd787"
  , "panelTitle.activeForeground"                         : "#d7d7d7"
  , "panelTitle.inactiveForeground"                       : "#c6c6c6"
  , "panelTitle.border"                                   : "#262626"
  //, "panelSectionHeader.background"                       : "#afd787"
  //, "panelTitleBadge.foreground"                          : "#262626"

  , "statusBar.background"                                : "#005f87"
  , "statusBar.foreground"                                : "#c6c6c6"
  , "statusBar.debuggingBackground"                       : "#875faf"
  , "statusBarItem.remoteBackground"                      : "#005f87"

  , "banner.background"                                   : "#ffd787"

  , "list.activeSelectionBackground"                      : "#005f5f"
  , "list.activeSelectionForeground"                      : "#c6c6c6"
  , "list.inactiveSelectionBackground"                    : "#262626"
  , "list.inactiveSelectionForeground"                    : "#005f87"
  , "list.hoverBackground"                                : "#005f5f5f"
  , "list.hoverForeground"                                : "#c6c6c6"
  , "list.dropBackground"                                 : "#ffd787"

  , "list.highlightForeground"                            : "#afd787"
  , "list.focusBackground"                                : "#005f87"

  , "button.background"                                   : "#005f5f"
  , "button.foreground"                                   : "#c6c6c6"

  , "debugIcon.breakpointForeground"                      : "#ff5f87"
  , "debugIcon.startForeground"                           : "#afd787"
  , "debugIcon.stopForeground"                            : "#d787af"
  , "debugIcon.stepIntoForeground"                        : "#87afd7"
  , "debugIcon.stepOverForeground"                        : "#87afd7"
  , "debugIcon.stepOutForeground"                         : "#87afd7"
  , "debugIcon.continueForeground"                        : "#afd787"
  , "debugIcon.restartForeground"                         : "#87d7af"
  , "editor.inlineValuesBackground"                       : "#303030"
  , "editor.inlineValuesForeground"                       : "#ffaf87"
  , "editor.stackFrameHighlightBackground"                : "#303030"

  , "gitDecoration.addedResourceForeground"               : "#87af5f"
  , "gitDecoration.modifiedResourceForeground"            : "#ffaf87"
  , "gitDecoration.untrackedResourceForeground"           : "#87af5f"

  , "scmGraph.foreground1"                                : "#ffaf87"
  , "scmGraph.historyItemBaseRefColor"                    : "#d75f87"
  , "scmGraph.historyItemRefColor"                        : "#005f5f"
  , "scmGraph.historyItemRemoteRefColor"                  : "#d75f87"

  //, "scmGraph.historyItemHoverDefaultLabelBackground"     : "#00875f"
  //, "scmGraph.historyItemHoverDefaultLabelForeground"     : "#c6c6c6"
  //, "scmGraph.foreground2"                                : "#d787af"
  //, "scmGraph.foreground3"                                : "#d787af"
  //, "scmGraph.foreground4"                                : "#d787af"
  //, "scmGraph.foreground5"                                : "#d787af"

  , "minimap.background"                                  : "#303030"
  , "minimap.errorHighlight"                              : "#ff5f87"
  , "minimap.warningHighlight"                            : "#ffaf87"
  , "minimap.selectionHighlight"                          : "#87d7afaf"

  , "inputValidation.warningBackground"                   : "#ff5f87"
  , "inputValidation.warningForeground"                   : "#c6c6c6"
  , "inputValidation.warningBorder"                       : "#c6c6c6"
  , "inputValidation.errorBackground"                     : "#262626"
  , "inputValidation.errorForeground"                     : "#ff5f87"
  , "inputValidation.errorBorder"                         : "#ff5f87"

  , "problemsErrorIcon.foreground"                        : "#ff5f87"
  , "problemsWarningIcon.foreground"                      : "#ffaf87"
  , "problemsInfoIcon.foreground"                         : "#ffaf87"

  , "symbolIcon.arrayForeground"                          : "#87af5f"
  //, "symbolIcon.booleanForeground"                        : "#"
  , "symbolIcon.classForeground"                          : "#87d7af"
  //, "symbolIcon.colorForeground"                          : "#"
  , "symbolIcon.constantForeground"                       : "#ffaf87"
  , "symbolIcon.constructorForeground"                    : "#af87d7"
  //, "symbolIcon.enumeratorForeground"                     : "#"
  //, "symbolIcon.enumeratorMemberForeground"               : "#"
  //, "symbolIcon.eventForeground"                          : "#"
  //, "symbolIcon.fieldForeground"                          : "#"
  //, "symbolIcon.fileForeground"                           : "#"
  //, "symbolIcon.folderForeground"                         : "#"
  , "symbolIcon.functionForeground"                       : "#87af5f"
  //, "symbolIcon.interfaceForeground"                      : "#"
  //, "symbolIcon.keyForeground"                            : "#"
  //, "symbolIcon.keywordForeground"                        : "#"
  , "symbolIcon.methodForeground"                         : "#87af5f"
  , "symbolIcon.moduleForeground"                         : "#ffaf87"
  //, "symbolIcon.namespaceForeground"                      : "#"
  //, "symbolIcon.nullForeground"                           : "#"
  //, "symbolIcon.numberForeground"                         : "#"
  //, "symbolIcon.objectForeground"                         : "#"
  //, "symbolIcon.operatorForeground"                       : "#"
  //, "symbolIcon.packageForeground"                        : "#"
  , "symbolIcon.propertyForeground"                       : "#87d7af"
  //, "symbolIcon.referenceForeground"                      : "#"
  //, "symbolIcon.snippetForeground"                        : "#"
  , "symbolIcon.stringForeground"                         : "#5fafaf"
  , "symbolIcon.structForeground"                         : "#ffaf87"
  //, "symbolIcon.textForeground"                           : "#ffaf87"
  //, "symbolIcon.typeParameterForeground"                  : "#"
  , "symbolIcon.unitForeground"                           : "#d787af"
  , "symbolIcon.variableForeground"                       : "#87afd7"

  }

, "tokenColors":
  [ {}

  , { "name"  : "Foreground"
    , "scope": [ ""
      , "source"
      , "text.html" // Comment specifier
      , "variable.parameter"
      , "meta.body.function.definition"
      , "meta.function.definition"
      , "meta.function-call.arguments"
      , "punctuation"
      , "punctuation.definition.string"
      , "punctuation.definition.string.begin.python"
      , "punctuation.support.type.property-name"
      , "punctuation.separator.arguments"
      , "punctuation.separator.array"
      , "punctuation.separator.dictionary.pair"
      , "punctuation.separator.key-value"             // class="value"
      , "string.interpolated"
      , ""
      ], "settings": { "foreground": "#c6c6c6" }
  }

  , { "name"  : "Foreground Bold"
    , "scope": [ ""
      , "keyword.control.ternary" // Java - : in Circle: "circle"
      , ""
      ], "settings": { "foreground": "#d7d7d7" }
    }

  , { "name"  : "Color 1: Pink"
    , "scope" : [ ""
      , "entity.name.function.preprocessor"
      , "entity.name.operator"
      , "heading.1.markdown"
      , "heading.1.quarto"
      , "keyword.control"               // switch, case, break, if, for
      , "keyword.other.directive.using" // using a = float
      , "keyword.other.operator"
      , "keyword.other.using"
      , ""
      ] , "settings": { "foreground": "#d787af" }
    }

  , { "name"  : "Color 9: Bright Pink"
    , "scope" : [ ""
      , "invalid"
      , ""
      ], "settings": { "foreground": "#ff5f87" }
    }

  , { "name"  : "Color 2: Green"
    , "scope" : [ ""

      , "heading.3.markdown"
      , "heading.3.quarto"
      , "keyword.operator"
      , "keyword.operator.assignment"
      , "keyword.operator.comparison"
      , "punctuation.definition.constant"         // # in #ff0000
      , "punctuation.definition.link.description"
      , "punctuation.separator.annotation.result" // -> in def f() -> int:
      ]
    , "settings": { "foreground": "#afd787" }
    }

  , { "name"  : "Color 10: Dark Green"
    , "scope" : [ ""
      , "keyword.control.directive"
      , "keyword.control.import"
      , "keyword.other.export"
      , "keyword.other.import"
      , "storage.type.modifier"        // public
      , ""
      ]
    , "settings": { "foreground": "#87af5f"}
    }

  , { "name"  : "Color 3: Yellow"
    , "scope" : [ ""
      , "constant.language"
      , "heading.2.markdown"
      , "heading.2.quarto"
      , "keyword"
      , "keyword.operator"
      , "keyword.operator.cast"                       // static_cast
      , "keyword.operator.delete"
      , "keyword.operator.new"
      , "meta.separator"
      , "storage.modifier"                            // const, static
      , "support.constant.property-value.css"
      , ""
      , ""
      ], "settings": { "foreground": "#ffaf87"}
    }

  , { "name": "Color 11: Yellow Bold"
    , "scope": [""
      , "constant.character.escape"    // \n in "\n"
      , "constant.other.placeholder"   // %02X in string
      , "storage.type.format"          // 2x in {a:2x}

      , "entity.name.operator.custom-literal"
      , "keyword.other.unit"
      ], "settings": { "foreground": "#ffd787" }
    }

 , { "name"  : "Color 4: Blue"
    , "scope" : [ ""
      //, "entity.name.variable"
      , "heading.4.markdown"
      , "heading.4.quarto"
      , "meta.member.access.python"
      , "markup.list.numbered"
      , "variable.other.object"
      , "variable.other.property" // b in a.b
      , "support.type.property-name"
      , ""
      ], "settings": { "foreground": "#87afd7"}
    }

  , { "name"  : "Color 12: Bright Blue"
    , "scope" : [ ""
      , "constant.other.placeholder"
      , "entity.name.tag"
      , "entity.name.type.parameter"
      , "markup.underline.link.image"
      , "meta.definition.variable.name"
      , "meta.method.return-type"
      , "support.type"
      , "storage.type.built-in"                     // int
      , "storage.type.function"                     // def in python
      , "storage.type.primitive"                    // int
      , "storage.type.string"                       // f in f'string'
      , "storage.type.template.argument.typename"   // public
      , "support.variable"
      , ""
      ], "settings": { "foreground": "#00afd7" }
    }

  , { "name"  : "Color 5: Violet"
    , "scope": [ ""
      , "entity.name.scope-resolution"
      , "heading.5.markdown"
      , "heading.5.quarto"
      , "storage.type.class"
      , "storage.type.enum"
      , "storage.type.namespace.definition"
      , "storage.type.struct"
      , "storage.type.template"
      , "storage.type.enum.enum-key.class.cpp"
      , "variable.language.special.self"          // python
      , "variable.language.this"                  // java
      , "variable.other.constant"
      , "entity.name.namespace"
      , "keyword.control.import.shortcode"
      , ""
      ], "settings": { "foreground": "#af87d7" }
    }

  , { "name"  : "Color 13: Dark Violet"
    , "scope" : [ ""
      , "constant.numeric"
      , "constant.other.color.rgb-value"
      , "constant.other.rgb-value"
      , "string"
      , ""
      ], "settings": { "foreground": "#af87af"}
    }

  , { "name"  : "Color 6: Cyan"
    , "scope" : [ ""
      , "entity.name.type"
      , "entity.other"
      ]
    , "settings": { "foreground": "#87d7af" }
    }

  , { "name"  : "Color 14: Dark Cyan"
    , "scope" : [ ""
      , "support.function"
      , "entity.name.function"
      , "meta.function-call"

      ], "settings": { "foreground": "#5fafaf" }
    }

  , { "name"  : "Color 0: Dark Grey"
    , "scope": [ ""
      , "comment"
      , "punctuation.definition.comment" // Comment specifier e.g. //
      , ""
      ], "settings": { "foreground": "#6c6c6c" }
    }

  , { "name"  : "CSS property value"
    , "scope" : [ ""
      , "support.constant.property-value"
      , "support.constant.font-name"
      , "support.constant.media-type"
      , "support.constant.media"
      , "support.constant.color"
      ]
    , "settings": { "foreground": "#00afd7" }
    }

  , { "name"  : "Regular expression groups"
    , "scope" : [ ""
      , "punctuation.definition.group.regexp"
      , "punctuation.definition.group.assertion.regexp"
      , "punctuation.definition.character-class.regexp"
      , "punctuation.character.set.begin.regexp"
      , "punctuation.character.set.end.regexp"
      , "keyword.operator.negation.regexp"
      , "support.other.parenthesis.regexp"
      ]
    , "settings": { "foreground": "#d75fd7" }
    }

  , { "scope": [
        "constant.character.character-class.regexp"
      , "constant.other.character-class.set.regexp"
      , "constant.other.character-class.regexp"
      , "constant.character.set.regexp"
      ]
    , "settings": { "foreground": "#d787af" }
    }

  , { "scope": [
        "keyword.operator.or.regexp"
      , "keyword.control.anchor.regexp"
      ]
    , "settings": { "foreground": "#DCDCAA" }
    }

  , { "scope": "keyword.operator.quantifier.regexp"
    , "settings": { "foreground": "#d7ba7d" }
    }

  ]

  // These properties override tokenColors
, "semanticTokenColors":
  {
    "enumMember"    : "#ffaf87" // Class member variables
  //"variable"      : "#87afd7"
  //, "property"      : "#87afd7" // Class member variables
  //, "parameter"     : "#87afd7" // Function parameters
  //, "enumMember"    : "#87afd7" // Class member variables
  //, "macro"         : "#d0d0d0" // PI in #define PI
  //, "type"          : "#87d7d7" // Don't set this
  }


}

